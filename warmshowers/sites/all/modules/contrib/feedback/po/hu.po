#: feedback.module:31
msgid "Configure the feedback page"
msgstr "Visszajelzés oldal beállítása"

#: feedback.module:70;86
msgid "Feedback page name"
msgstr "Visszajelzés oldal neve"

#: feedback.module:70
msgid "Feedback page location"
msgstr "Visszajelzés oldal helye"

#: feedback.module:74
msgid "delete"
msgstr "törlés"

#: feedback.module:76
msgid "edit"
msgstr "szerkesztés"

#: feedback.module:93
msgid "Add page"
msgstr "Oldal hozzáadása"

#: feedback.module:97
msgid "Add a further feedback page"
msgstr "További visszajelzés oldal hozzáadása"

#: feedback.module:106
msgid "This page already exists."
msgstr "Ez az oldal már létezik"

#: feedback.module:112
msgid "Your feedback page has been added."
msgstr "A visszajelzés oldal hozzá lett adva."

#: feedback.module:123
msgid "Default Email Address"
msgstr "Alapértelmezett email cím"

#: feedback.module:127
msgid "The email address which should receive all form submissions"
msgstr "Az email cím, amely az összes visszajelzést fogadja"

#: feedback.module:132
msgid "Title of the feedback page"
msgstr "A visszajelzés oldal címe"

#: feedback.module:140
msgid "Instructions"
msgstr "Útmutató"

#: feedback.module:144
msgid "The instructions that will be displayed for the user on how to fill the form"
msgstr "Útmutató arról, hogy a felhasználó hogyan töltse ki az űrlapot"

#: feedback.module:148;323
msgid "Subject"
msgstr "Tárgy"

#: feedback.module:152
msgid "Subject Prefix"
msgstr "Tárgy előtag"

#: feedback.module:156
msgid "The prefix that should be added before the subject on each email"
msgstr "Az előtag, amely minden email címében megjelenik"

#: feedback.module:160
msgid "Include a subject textfield"
msgstr "Tartalmazza a tárgy beviteli mezejét"

#: feedback.module:166
msgid "Categories"
msgstr "Kategóriák"

#: feedback.module:170
msgid "Put each subject category on a separate line or separate them by commas.  No HTML allowed."
msgstr "Az egyes kategóriákat vesszővel kell elválasztani, vagy új sorba kell tenni. A HTML jelölések használata nem megengedett."

#: feedback.module:170
msgid "Leave this field empty to disable this feature."
msgstr "A lehetőség kikapcsolásához ezt a mezőt üresen kell hagyni."

#: feedback.module:174
msgid "Fields to include on the form"
msgstr "Az űrlapon megjelenő mezők"

#: feedback.module:178
msgid "Sender Name"
msgstr "Feladó neve"

#: feedback.module:184
msgid "Postal Address"
msgstr "Levelezési cím"

#: feedback.module:190;480
msgid "Phone Number"
msgstr "Telefonszám"

#: feedback.module:196
msgid "Message Body"
msgstr "Üzenet törzse"

#: feedback.module:203
msgid "Miscellaneous Settings"
msgstr "Egyéb beállítások"

#: feedback.module:207
msgid "Hourly threshold"
msgstr "Óránkénti limit"

#: feedback.module:210
msgid "The maximum number of form submissions a user can perform per hour."
msgstr "Egy adott felhasználó által óránk beküldhető visszajelzések száma."

#: feedback.module:214
msgid "Log all feedback attempts to watchdog"
msgstr "Naplózza az összes visszajelzést"

#: feedback.module:220
msgid "Save configuration"
msgstr "Beállítások mentése"

#: feedback.module:228
msgid "The configuration options have been saved."
msgstr "A beállítások el lettek mentve."

#: feedback.module:236;243
msgid "Delete"
msgstr "Törlés"

#: feedback.module:238
msgid "The feedback page has been deleted."
msgstr "A visszajelzés oldal törölve lett."

#: feedback.module:242
msgid "Are you sure you want to delete the feedback page \"!page\"?"
msgstr "Biztos, hogy a visszajelzés oldal törölve legyen: „!page” ?"

#: feedback.module:243
msgid "Cancel"
msgstr "Mégsem"

#: feedback.module:270
msgid "You cannot send more than !number messages per hour. Please try again later."
msgstr "Nem küldhető több levél !number darabnál óránként. Az újabb visszajelzést később lehet megpróbálni"

